{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "plugin:react/jsx-runtime"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [0, "unix"],
        "quotes": ["error", "double"],
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "import/no-extraneous-dependencies": 0
    }
}